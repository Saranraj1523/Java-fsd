package smallestnumber;
import java.util.Arrays;

public class unsortedlist {
	public static int kthsmall(int[] arr,int k){
		Arrays.sort(arr);
		return (k-1);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a[]={3,7,12,98,54,5};
		int k=4;
		System.out.println(kthsmall(a, k));
	}
	

}
